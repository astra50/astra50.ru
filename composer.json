{
    "name": "astra50/astra50.ru",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "3.3.9",
        "doctrine/doctrine-bundle": "^1.7.0",
        "doctrine/doctrine-cache-bundle": "^1.3.0",
        "doctrine/doctrine-migrations-bundle": "^1.2.1",
        "symfony/swiftmailer-bundle": "^2.6.3",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.5.0",
        "sensio/distribution-bundle": "^5.0.21",
        "sensio/framework-extra-bundle": "^3.0.27",
        "ramsey/uuid-doctrine": "^1.4.1",
        "stof/doctrine-extensions-bundle": "^1.2.2",
        "friendsofsymfony/user-bundle": "^2.0.1",
        "hwi/oauth-bundle": "^0.5.3",
        "knplabs/knp-menu-bundle": "^2.1.3",
        "white-october/pagerfanta-bundle": "^1.0.8",
        "twig/extensions": "^1.5.1",
        "guzzlehttp/guzzle": "^6.3.0",
        "grachevko/enum": "^0",
        "sentry/sentry-symfony": "^0.8.6"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.4.0",
        "symfony/phpunit-bridge": "^3.3.8",
        "doctrine/data-fixtures": "^1.2.2",
        "phpunit/phpunit": "^6.2.4",
        "friendsofphp/php-cs-fixer": "^2.5.0",
        "symfony/dotenv": "^3.3",
        "phpstan/phpstan": "^0.8.5",
        "phpstan/phpstan-doctrine": "^0.8.0",
        "fzaninotto/faker": "^1.7"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "symlink",
        "config": {
            "preferred-install": "dist",
            "sort-packages": true
        }
    }
}
