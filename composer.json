{
    "name": "astra50/astra50.ru",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "cebe/markdown": "^1.1.2",
        "doctrine/doctrine-bundle": "^1.8.1",
        "doctrine/doctrine-cache-bundle": "^1.3.2",
        "doctrine/doctrine-migrations-bundle": "^1.3.1",
        "doctrine/orm": "^2.6",
        "friendsofsymfony/user-bundle": "^2.0.2",
        "fxp/composer-asset-plugin": "^1.4.2",
        "grachevko/enum": "^0.10",
        "hwi/oauth-bundle": "^0.6.1",
        "knplabs/knp-menu-bundle": "^2.2.1",
        "npm-asset/bootstrap-markdown": "dev-master#a20ef2d",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.8.1",
        "sensio/distribution-bundle": "^5.0.21",
        "sensio/framework-extra-bundle": "^5.1.5",
        "sentry/sentry-symfony": "^2.0.1",
        "stof/doctrine-extensions-bundle": "^1.3.0",
        "symfony/monolog-bundle": "^3.1.2",
        "symfony/swiftmailer-bundle": "^3.1.6",
        "symfony/symfony": "^3.4.4",
        "twig/extensions": "^1.5.1",
        "white-october/pagerfanta-bundle": "^1.1.2"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0.2",
        "friendsofphp/php-cs-fixer": "^2.10.2",
        "fzaninotto/faker": "^1.7.1",
        "lookyman/phpstan-symfony": "^0.2.0",
        "phpstan/phpstan": "^0.9.2",
        "phpstan/phpstan-doctrine": "^0.9.1",
        "phpstan/phpstan-phpunit": "^0.9.4",
        "phpunit/phpunit": "^6.5.6",
        "symfony/dotenv": "^3.3",
        "symfony/phpunit-bridge": "^3.4.4"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "symlink"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "public/assets/vendor"
            }
        }
    }
}
